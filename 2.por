programa {
  funcao inicio() {
    cadeia resposta
    escreva("escolha as opçoes esquerda ou direita: ")
    leia(resposta)
    se (resposta=="esquerda")
    escreva("Parabens você encontrou um tesouro! ")
   senao se (resposta=="direita"){
   escreva("Você caiu em uma armadilha! ")
   }senao
   escreva("escolha invalida. tente novamente")
  }
  }

  
